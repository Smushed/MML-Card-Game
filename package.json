{
  "name": "mml-card-be",
  "version": "1.0.0",
  "description": "Backend for the MML Card Game",
  "main": "server.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc",
    "generate": "rm -r ./client/src/models && cp -r ./models ./client/src/models",
    "client": "cd client && npm start",
    "hot": "nodemon",
    "start": "node dist/server.js",
    "start-dev": "ts-node src/server.ts",
    "dev": "concurrently \"nodemon --ignore client\" \"npm run client\""
  },
  "author": "Kevin Flerlage",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^6.9.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.11.19",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1"
  }
}
